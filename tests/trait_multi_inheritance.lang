// Test: Multiple trait inheritance
// trait Print<t> : Stringify<t>, Cmp<t>

// Base traits
trait Stringify<t> {
    fun stringify(val: t) -> String
}

trait Cmp<t> {
    fun cmp(a: t, b: t) -> Bool
}

// Derived trait inheriting from both
trait Print<t> : Stringify<t>, Cmp<t> {
    fun format(val: t) -> String
}

// Implement Stringify for Int
instance Stringify Int {
    fun stringify(val: Int) -> String {
        "Int"
    }
}

// Implement Cmp for Int
instance Cmp Int {
    fun cmp(a: Int, b: Int) -> Bool {
        a == b
    }
}

// Implement Print for Int
instance Print Int {
    fun format(val: Int) -> String {
        "Formatted: Int"
    }
}

// Test usage
print(stringify(42))
print(cmp(1, 1))
print(cmp(1, 2))
print(format(100))

