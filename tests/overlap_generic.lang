// Test overlapping instances detection
// instance MyCmp A - polymorphic instance for all types
// instance MyCmp Int - specific instance for Int
// These overlap because Int is included in "all types A"

trait MyCmp<t> {
    fun cmp(x: t, y: t) -> Bool
}

instance MyCmp a {
    fun cmp(x: a, y: a) -> Bool { true }
}

instance MyCmp Int {
    fun cmp(x: Int, y: Int) -> Bool { true }
}

