trait Printable<t> {
  fun pretty(val: t) -> String
}

instance Printable (List<Int>) {
  fun pretty(val: List<Int>) -> String {
    "List of Ints"
  }
}

l = [1, 2]
print(pretty(l))

instance Printable (Int, Int) {
  fun pretty(val: (Int, Int)) -> String {
    "Tuple of Ints"
  }
}
t = (1, 2)
print(pretty(t))
