// Test: Constraint generics error
// String does not implement Printable

trait Printable<t> {
    fun pretty(val: t) -> String
}

instance Printable Int {
    fun pretty(val: Int) -> String {
        "int"
    }
}

// Constrained function - t must implement Printable
fun display<t: Printable>(x: t) -> String {
    pretty(x)
}

// ERROR: String (List Char) does not implement Printable
print(display("hello"))
