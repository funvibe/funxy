package bugweb

// Types file - comes first alphabetically
import "lib/json" (Json)

type ResponseBody =
    BodyString String
    | BodyJson Json
    | BodyEmpty

type alias Response = {
    status: Int,
    headers: List<(String, String)>,
    body: ResponseBody
}

trait Responder<t> {
    fun toResponse(val: t) -> Response
}
