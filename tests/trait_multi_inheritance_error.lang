// Test: Error when one of multiple super traits is missing

trait Stringify<t> {
    fun stringify(val: t) -> String
}

trait Cmp<t> {
    fun cmp(a: t, b: t) -> Bool
}

// Print requires both Stringify and Cmp
trait Print<t> : Stringify<t>, Cmp<t> {
    fun format(val: t) -> String
}

// Only implement Stringify, NOT Cmp
instance Stringify Int {
    fun stringify(val: Int) -> String {
        "Int"
    }
}

// ERROR: Cmp Int is not implemented
instance Print Int {
    fun format(val: Int) -> String {
        "Formatted"
    }
}

print("should not reach")
