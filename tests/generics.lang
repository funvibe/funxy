// Explicit Generics
fun myId(x: t) -> t {
    x
}

// Call with Int
i = myId(5)
print(i) // 5

// Call with String (List Char)
s = myId("hello")
print(s) // "hello"

// Generic Function with List
fun first(list: List<t>) -> Option<t> {
    if len(list) > 0 {
        Some(list[0])
    } else {
        None
    }
}

f1 = first([1, 2, 3])
print(f1) // Some(1)

f2 = first([])
print(f2) // None

// Generic Wrapper Struct
type alias Box<t> = { value: t }

fun makeBox(v: t) -> Box<t> {
    { value: v }
}

b = makeBox(42)
print(b.value) // 42

