trait Container<c> {
    fun wrap(x: a) -> c<a>
    fun unwrap(cx: c<a>) -> a
}

instance Container<Option> {
    fun wrap(x: a) -> Option<a> { Some(x) }
    fun unwrap(cx: Option<a>) -> a {
        match cx {
            Some(v) -> v
            None -> panic("empty")
        }
    }
}

opt = wrap(42)
print(opt)
